{
  "name": "postal-zip-api",
  "version": "0.0.0-alpha",
  "description": "A zip code list management REST API",
  "main": "dist/server.js",
  "scripts": {
    "start": "rimraf dist && tsc && CLI=true node -r dotenv/config scripts/serve.js",
    "insert": "CLI=true node -r dotenv/config scripts/insert.js",
    "delete": "CLI=true node -r dotenv/config scripts/delete.js",
    "has": "CLI=true node -r dotenv/config scripts/has.js",
    "display": "CLI=true node -r dotenv/config scripts/display.js",
    "test": "node -r dotenv/config jest"
  },
  "keywords": [
    "StackSource",
    "zip",
    "codes",
    "REST",
    "API",
    "coding",
    "challenge"
  ],
  "author": "Augustus Buonviri",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.14.0",
    "@types/node-fetch": "^2.5.10",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "eslint": "^7.29.0",
    "jest": "^27.0.6",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@types/express": "^4.17.12",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1"
  }
}
